<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/tailwindcss@2.2.19/dist/tailwind.min.css" rel="stylesheet">
    <title>Document</title>
</head>

<body>
    <% include('nav.ejs') %>
    <section class="text-gray-600 body-font overflow-hidden">
        <div class="container px-5 py-24 mx-auto">
            <div class="-my-8 divide-y-2 divide-gray-100">
                <div class="flex flex-wrap -mx-4">
                    <% candidates.forEach(candidate => { %>
                    <div class="p-4 md:w-1/3">
                        <div class="h-full border-2 border-gray-200 border-opacity-60 rounded-lg overflow-hidden">
                            <div class="p-6">
                                <h2 class="text-2xl font-medium text-gray-900 title-font mb-2">ID: <%= candidate.id %></h2>
                                <p class="leading-relaxed mb-3">Party: <%= candidate.party %></p>
                                <p class="leading-relaxed mb-3">Total Votes: <%= candidate.totalvote %></p>
                            </div>
                        </div>
                    </div>
                    <% }); %>
                </div>
                <div>
                    <h2 class="mt-8">Winning Party: <%= winningParty %></h2>
                </div>
            </div>
        </div>
    </section>
    <style>
        .vss {
            color: #01D293;
        }

        .vaa {
            color: white;
        }
    </style>
    <% include('style.ejs') %>

</body>

</html>
